{
  "private": "true",
  "name": "@pawhaven/core-service",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "pnpm run prisma:generate-mongodb",
    "dev": "nest start --watch",
    "build": "pnpm prisma:generate-mongodb && nest build",
    "turbo:build": "turbo run build --filter=@pawhaven/core-service --env-mode=loose",
    "start": "node dist/main.js",
    "prisma:generate-mongodb": "prisma generate --schema=./src/prisma/mongodb/schema.prisma",
    "prisma:db-push": "prisma db push --schema=./src/prisma/mongodb/schema.prisma",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.1",
    "@grpc/proto-loader": "^0.8.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@pawhaven/backend-core": "workspace:*",
    "@prisma/client": "^6.19.1",
    "@types/express": "^5.0.3",
    "axios": "^1.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "cross-env": "^10.1.0",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "helmet": "^8.1.0",
    "js-yaml": "^4.1.0",
    "mongoose": "^8.19.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.6",
    "@pawhaven/eslint-config": "workspace:*",
    "@pawhaven/tsconfig": "workspace:*",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.4",
    "prisma": "^6.19.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
